{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "wbcom-essential/members-carousel",
  "version": "1.0.0",
  "title": "Members Carousel",
  "category": "starter-buddypress",
  "icon": "slides",
  "description": "Display BuddyPress members in an interactive carousel.",
  "keywords": [
    "members",
    "carousel",
    "slider",
    "buddypress",
    "community"
  ],
  "textdomain": "wbcom-essential",
  "example": {},
  "attributes": {
    "sortType": {
      "type": "string",
      "default": "newest"
    },
    "totalMembers": {
      "type": "number",
      "default": 12
    },
    "showLastActive": {
      "type": "boolean",
      "default": true
    },
    "slidesToShow": {
      "type": "number",
      "default": 4
    },
    "slidesToShowTablet": {
      "type": "number",
      "default": 2
    },
    "slidesToShowMobile": {
      "type": "number",
      "default": 1
    },
    "slidesToScroll": {
      "type": "number",
      "default": 1
    },
    "navigation": {
      "type": "string",
      "default": "both"
    },
    "autoplay": {
      "type": "boolean",
      "default": true
    },
    "pauseOnHover": {
      "type": "boolean",
      "default": true
    },
    "autoplaySpeed": {
      "type": "number",
      "default": 5000
    },
    "infiniteLoop": {
      "type": "boolean",
      "default": true
    },
    "animationSpeed": {
      "type": "number",
      "default": 500
    },
    "spaceBetween": {
      "type": "number",
      "default": 30
    },
    "cardBgColor": {
      "type": "string",
      "default": "#ffffff"
    },
    "cardBorderRadius": {
      "type": "number",
      "default": 8
    },
    "cardShadow": {
      "type": "boolean",
      "default": true
    },
    "nameColor": {
      "type": "string",
      "default": "#122B46"
    },
    "metaColor": {
      "type": "string",
      "default": "#A3A5A9"
    },
    "arrowColor": {
      "type": "string",
      "default": "#122B46"
    },
    "dotColor": {
      "type": "string",
      "default": "#122B46"
    },
    "pauseOnInteraction": {
      "type": "boolean",
      "default": false
    },
    "direction": {
      "type": "string",
      "default": "horizontal"
    },
    "effect": {
      "type": "string",
      "default": "slide"
    },
    "enableKeyboard": {
      "type": "boolean",
      "default": true
    },
    "grabCursor": {
      "type": "boolean",
      "default": true
    }
  },
  "supports": {
    "html": false,
    "align": [
      "wide",
      "full"
    ],
    "spacing": {
      "margin": true,
      "padding": true
    }
  },
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "viewScript": [
    "file:./view.js",
    "swiper"
  ],
  "render": "file:./render.php"
}